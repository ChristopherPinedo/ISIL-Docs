  <DataSets>
    <DataSet Name="MontoEnviosTienda">
      <Query>
        <DataSourceName>QhatuPERU</DataSourceName>
        <CommandText>SELECT        TIENDA.Direccion, GUIA_ENVIO.NumGuia, CONVERT(CHAR(10), GUIA_ENVIO.FechaSalida, 102) AS Fecha, 
                         SUM(GUIA_DETALLE.CantidadEnviada * GUIA_DETALLE.PrecioVenta) AS Monto
FROM            GUIA_ENVIO INNER JOIN
                         TIENDA ON GUIA_ENVIO.CodTienda = TIENDA.CodTienda INNER JOIN
                         GUIA_DETALLE ON GUIA_ENVIO.NumGuia = GUIA_DETALLE.NumGuia
GROUP BY TIENDA.Direccion, GUIA_ENVIO.NumGuia, GUIA_ENVIO.FechaSalida
ORDER BY TIENDA.Direccion, Fecha</CommandText>
      </Query>
